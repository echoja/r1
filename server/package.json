{
  "name": "server",
  "version": "0.0.1",
  "description": "Research S Typescript Server",
  "main": "index.js",
  "scripts": {
    "clean": "rm -fr dist",
    "start-dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --watch ./src/**/*.pug,./src/**/*.graphql --trace-warnings --trace-uncaught -- ./src/index.ts",
    "start-ts-node": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=production node ./bundle/index.js",
    "build-simple": "tsc --project tsconfig.json",
    "rollup": "rollup -c",
    "build": "npm run build-simple && npm run rollup"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/connect-history-api-fallback": "^1.3.4",
    "@types/express": "^4.17.11",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "nodemon": "^2.0.7",
    "rollup": "^2.49.0",
    "terser": "^5.7.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "tedious": "^11.0.9",
    "tsconfig-paths": "^3.9.0"
  }
}
